<style>
    body{
        background-image:url(/images/background.jpg);
		font-family: 'Architects Daughter', cursive;
letter-spacing: 2px;
        
        background-repeat: no-repeat;
        background-size:     cover;  
    background-position: center center;  
    
	}
	.google{
   width:40%;
   margin-bottom:20px;
	}
	


</style>
<div class="body hey">
	<div class="login-wrap hey" >
		<div class="login-html" style="background-color:rgba(0,0,0,0.65)">
			<input id="tab-1" type="radio" name="tab" class="sign-in" checked><label for="tab-1" class="tab">Sign Up</label>
			<input id="tab-2" type="radio" name="tab" class="for-pwd"><label for="tab-2" style="color:white"  class="tab">Other Options</label>
			<div class="login-form">
			<form action="/register" method="POST">
				<div class="sign-in-htm">
					<div class="group">
						<label for="user" class="label">Username or Email</label>
						<input id="user" name="username" type="text" class="input" placeholder="Username">
					</div>
					<div class="group">
						<label for="pass" class="label">Password</label>
						<input id="pass" type="password" name="password" class="input" data-type="password" placeholder="Password">
					</div>
					<div class="group">
						<label for="admin" class="label">AdminCode</label>
						<input id="admin" type="password" name="adminCode" class="input" placeholder="AdminCode">
					</div>
					<div class="group bigbox">
						<input type="submit" class="button" value="Sign Up">
					</div>
					<div class="hr"></div>
				</div>
			</form>
				<div class="for-pwd-htm">
					<div class="group" style="text-align:center">
						<label for="user" class="label">Sign Up Using Your GMail Account</label>
						<!-- <input id="user" name="username" type="text" class="input"> -->
						<img class="google full-circle" src="data:image/svg+xml;base64," />
					</div>
					<div class="group">
						<input type="submit" class="button bigbox" value="Register Using GMail Account">
					</div>
					<div class="hr"></div>
				</div>
			
			</div>
		</div>
	</div>
</div>

<%- include("partials/footer") %>